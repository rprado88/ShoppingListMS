networks:
  network-rodrigo:
    driver: bridge
services:
  db:
    build:
      context: /Users/rodrigoprado/Projects/ShoppingListMS
      dockerfile: db/Dockerfile
    command: mysqld --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_DATABASE: ShoppingList
      MYSQL_PASSWORD: sa123
      MYSQL_ROOT_PASSWORD: docker
      MYSQL_USER: root
      TZ: America/Sao_Paulo
    image: docker-shopping-list-mysql-db
    networks:
      network-rodrigo: {}
    ports:
    - published: 3308
      target: 3306
    restart: always
  shoppinglistms.api:
    build:
      context: /Users/rodrigoprado/Projects/ShoppingListMS
      dockerfile: ShoppingListMS/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ShoppingListMS.Api
      target: base
    container_name: ShoppingListMS.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      TZ: America/Sao_Paulo
    image: shoppinglistms:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/ShoppingListMS.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      network-rodrigo: {}
    ports:
    - published: 5000
      target: 80
    tty: true
    volumes:
    - /Users/rodrigoprado/Projects/ShoppingListMS/ShoppingListMS:/app:rw
    - /Users/rodrigoprado/vsdbg/vs2017u5:/remote_debugger:rw
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/rodrigoprado/.nuget/packages:/root/.nuget/packages:ro
    - /Users/rodrigoprado/Projects/ShoppingListMS:/src:rw
version: '3.4'