networks:
  network-rodrigo:
    driver: bridge
services:
  db:
    build:
      context: /Users/rodrigoprado/Projects/ShoppingListMS
      dockerfile: db/Dockerfile
    command: mysqld --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_DATABASE: ShoppingList
      MYSQL_PASSWORD: sa123
      MYSQL_ROOT_PASSWORD: docker
      MYSQL_USER: root
      TZ: America/Sao_Paulo
    image: docker-shopping-list-mysql-db
    networks:
      network-rodrigo: null
    ports:
    - published: 3308
      target: 3306
    restart: always
  shoppinglistms.api:
    build:
      context: /Users/rodrigoprado/Projects/ShoppingListMS
      dockerfile: ShoppingListMS/Dockerfile
    environment:
      TZ: America/Sao_Paulo
    image: shoppinglistms
    networks:
      network-rodrigo: null
    ports:
    - published: 5000
      target: 80
version: '3.4'